{
    "$schema": "http://json-schema.org/schema#",
    "properties": {
        "original address": {
          "type": "string",
          "description": "Original address, entities separeted with a comma"
        },
        "city": {
            "type": "string",
            "description": "City"
        },
        "state": {
            "type": "string",
            "description": "State or province"
        },
        "country_code": {
            "$ref": "country_code.json"
        },
        "postal_code": {
            "type": "string",
            "description": "Postal code"
        },
        "longitude": {
            "type": "number",
            "description": "Longitude of the place"
        },
        "latitude": {
          "type": "number",
          "description": "Latitude of the place"
        }
    },
    "type": "object",
    "description": "Address"
}
