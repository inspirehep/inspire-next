{
    "$schema": "http://json-schema.org/schema#",
    "additionalProperties": false,
    "description": "Address",
    "properties": {
        "city": {
            "description": "City",
            "type": "string"
        },
        "country_code": {
            "$ref": "country_code.json"
        },
        "latitude": {
            "description": "Latitude of the place",
            "type": "number"
        },
        "longitude": {
            "description": "Longitude of the place",
            "type": "number"
        },
        "original address": {
            "description": "Original address, entities separeted with a comma",
            "type": "string"
        },
        "postal_code": {
            "description": "Postal code",
            "type": "string"
        },
        "state": {
            "description": "State or province",
            "type": "string"
        }
    },
    "type": "object"
}
