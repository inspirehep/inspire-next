{
    "mappings": {
        "journals": {
            "_all": {
                "enabled": true
            },
            "date_detection": false,
            "numeric_detection": false,
            "properties": {
                "$schema": {
                    "type": "string"
                },
                "_collections": {
                    "analyzer": "lowercase_analyzer",
                    "type": "string"
                },
                "_harvesting_info": {
                    "properties": {
                        "coverage": {
                            "type": "string"
                        },
                        "date_last_harvest": {
                            "format": "yyyy||yyyy-MM||yyyy-MM-dd",
                            "type": "date"
                        },
                        "last_seen_item": {
                            "type": "string"
                        },
                        "method": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "_private_notes": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "book_series": {
                    "type": "boolean"
                },
                "control_number": {
                    "type": "integer"
                },
                "date_ended": {
                    "format": "yyyy||yyyy-MM||yyyy-MM-dd",
                    "type": "date"
                },
                "date_started": {
                    "format": "yyyy||yyyy-MM||yyyy-MM-dd",
                    "type": "date"
                },
                "deleted": {
                    "type": "boolean"
                },
                "deleted_recids": {
                    "type": "integer"
                },
                "deleted_records": {
                    "properties": {
                        "$ref": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "doi_prefixes": {
                    "type": "string"
                },
                "inspire_categories": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "term": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "issns": {
                    "properties": {
                        "medium": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "journal_title": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "subtitle": {
                            "type": "string"
                        },
                        "title": {
                            "copy_to": [
                                "journalautocomplete",
                                "lowercase_journal_titles"
                            ],
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "journalautocomplete": {
                    "type": "string"
                },
                "legacy_creation_date": {
                    "format": "yyyy||yyyy-MM||yyyy-MM-dd",
                    "type": "date"
                },
                "license": {
                    "properties": {
                        "license": {
                            "type": "string"
                        },
                        "url": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "lowercase_journal_titles": {
                    "analyzer": "lowercase_analyzer",
                    "type": "string"
                },
                "new_recid": {
                    "type": "integer"
                },
                "new_record": {
                    "properties": {
                        "$ref": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "proceedings": {
                    "type": "boolean"
                },
                "public_notes": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "publisher": {
                    "type": "keyword"
                },
                "refereed": {
                    "type": "boolean"
                },
                "related_records": {
                    "properties": {
                        "curated_relation": {
                            "type": "boolean"
                        },
                        "recid": {
                            "type": "integer"
                        },
                        "record": {
                            "properties": {
                                "$ref": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "relation": {
                            "type": "string"
                        },
                        "relation_freetext": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "self": {
                    "properties": {
                        "$ref": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "self_recid": {
                    "type": "integer"
                },
                "short_title": {
                    "copy_to": [
                        "journalautocomplete",
                        "lowercase_journal_titles"
                    ],
                    "type": "string"
                },
                "title_suggest": {
                    "type": "completion"
                },
                "title_variants": {
                    "copy_to": [
                        "journalautocomplete",
                        "lowercase_journal_titles"
                    ],
                    "type": "string"
                },
                "urls": {
                    "properties": {
                        "description": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            }
        }
    },
    "settings": {
        "analysis": {
            "analyzer": {
                "lowercase_analyzer": {
                    "filter": "lowercase",
                    "tokenizer": "keyword",
                    "type": "custom"
                }
            }
        }
    }
}
