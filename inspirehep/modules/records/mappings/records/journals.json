{
    "mappings": {
        "journals": {
            "_all": {
                "enabled": true
            },
            "date_detection": false,
            "numeric_detection": false,
            "properties": {
                "$schema": {
                    "type": "string"
                },
                "_collections": {
                    "type": "string"
                },
                "_private_notes": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "coden": {
                    "type": "string"
                },
                "control_number": {
                    "type": "integer"
                },
                "deleted": {
                    "type": "boolean"
                },
                "deleted_recids": {
                    "type": "integer"
                },
                "deleted_records": {
                    "properties": {
                        "$ref": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "history": {
                    "type": "string"
                },
                "issn": {
                    "properties": {
                        "comment": {
                            "type": "string"
                        },
                        "medium": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "journal_handling": {
                    "type": "string"
                },
                "journal_titles": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "subtitle": {
                            "type": "string"
                        },
                        "title": {
                            "copy_to": [
                                "journalautocomplete",
                                "lowercase_journal_titles"
                            ],
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "journalautocomplete": {
                    "type": "string"
                },
                "legacy_creation_date": {
                    "type": "string"
                },
                "license": {
                    "type": "string"
                },
                "license_urls": {
                    "properties": {
                        "description": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "lowercase_journal_titles": {
                    "analyzer": "lowercase_analyzer",
                    "type": "string"
                },
                "new_recid": {
                    "type": "integer"
                },
                "new_record": {
                    "properties": {
                        "$ref": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "peer_reviewed": {
                    "type": "boolean"
                },
                "public_notes": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "publisher": {
                    "type": "string"
                },
                "relation": {
                    "properties": {
                        "curated_relation": {
                            "type": "boolean"
                        },
                        "issn": {
                            "type": "string"
                        },
                        "recid": {
                            "type": "integer"
                        },
                        "record": {
                            "properties": {
                                "$ref": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "relation": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "self": {
                    "properties": {
                        "$ref": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "self_recid": {
                    "type": "integer"
                },
                "short_titles": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "subtitle": {
                            "type": "string"
                        },
                        "title": {
                            "copy_to": "journalautocomplete",
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "title_suggest": {
                    "payloads": true,
                    "type": "completion"
                },
                "title_variants": {
                    "properties": {
                        "source": {
                            "type": "string"
                        },
                        "subtitle": {
                            "type": "string"
                        },
                        "title": {
                            "copy_to": "lowercase_journal_titles",
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "urls": {
                    "properties": {
                        "description": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            }
        }
    },
    "settings": {
        "analysis": {
            "analyzer": {
                "lowercase_analyzer": {
                    "filter": "lowercase",
                    "tokenizer": "keyword",
                    "type": "custom"
                }
            }
        }
    }
}
